{
  "name": "next-rails",
  "version": "0.0.1",
  "description": "Nextjs boilerplate for CRUD operations using Typescript's decorators",
  "main": "server/server.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/danutzcodrescu/next-rails"
  },
  "scripts": {
    "dev": "nodemon server/server.ts",
    "build": "next build",
    "start": "next start",
    "lint": "tslint -p .",
    "lint-fix": "tslint -p . --fix",
    "style": "prettier --list-different --single-quote \"**/*.+(ts|tsx)\"",
    "style-fix": "prettier --write --single-quote \"**/*.+(ts|tsx)\"",
    "fake-server": "json-server --watch json-server/db.json --port 4200"
  },
  "keywords": [
    "nextjs",
    "typescript",
    "decorators",
    "crud"
  ],
  "author": "Danut Codrescu",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "lodash": "^4.17.11",
    "next": "^7.0.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.2.0",
    "@babel/plugin-proposal-decorators": "7.1.2",
    "@types/axios": "0.14.0",
    "@types/classnames": "2.2.6",
    "@types/express": "4.16.0",
    "@types/lodash": "4.14.119",
    "@types/next": "7.0.4",
    "@zeit/next-sass": "1.0.1",
    "@zeit/next-typescript": "1.1.1",
    "classnames": "2.2.6",
    "husky": "1.2.0",
    "json-server": "0.14.0",
    "node-sass": "4.10.0",
    "nodemon": "1.18.7",
    "prettier": "1.15.1",
    "reflect-metadata": "0.1.12",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.15.0",
    "typescript": "3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint && yarn run style"
    }
  }
}
